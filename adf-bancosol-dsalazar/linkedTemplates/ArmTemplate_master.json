{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-bancosol-dsalazar"
		},
		"ls_conn_dta_lake_banco_sol_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_conn_dta_lake_banco_sol'"
		},
		"ls_maquina_local_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_maquina_local'"
		},
		"ls_sql_database_banco_sol_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sql_database_banco_sol'"
		},
		"ls_sql_local_dirza_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'ls_sql_local_dirza'"
		},
		"ls_conn_dta_lake_banco_sol_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://dlsbancosoltraining.dfs.core.windows.net"
		},
		"ls_maquina_local_properties_typeProperties_host": {
			"type": "string",
			"defaultValue": "C:\\Users\\dirzasalazarmonaster\\Documents\\DATAFACTORY"
		},
		"ls_maquina_local_properties_typeProperties_userId": {
			"type": "string",
			"defaultValue": "dirzasalaza6d0a\\dirzasalazarmonaster"
		},
		"ls_keyvault_banco_sol_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://kv-bancosol-training.vault.azure.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-bancosol-dsalazar_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_conn_dta_lake_banco_sol_accountKey": {
						"value": "[parameters('ls_conn_dta_lake_banco_sol_accountKey')]"
					},
					"ls_sql_database_banco_sol_connectionString": {
						"value": "[parameters('ls_sql_database_banco_sol_connectionString')]"
					},
					"ls_maquina_local_password": {
						"value": "[parameters('ls_maquina_local_password')]"
					},
					"ls_sql_local_dirza_connectionString": {
						"value": "[parameters('ls_sql_local_dirza_connectionString')]"
					},
					"ls_conn_dta_lake_banco_sol_properties_typeProperties_url": {
						"value": "[parameters('ls_conn_dta_lake_banco_sol_properties_typeProperties_url')]"
					},
					"ls_keyvault_banco_sol_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_keyvault_banco_sol_properties_typeProperties_baseUrl')]"
					},
					"ls_maquina_local_properties_typeProperties_host": {
						"value": "[parameters('ls_maquina_local_properties_typeProperties_host')]"
					},
					"ls_maquina_local_properties_typeProperties_userId": {
						"value": "[parameters('ls_maquina_local_properties_typeProperties_userId')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "adf-bancosol-dsalazar_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'adf-bancosol-dsalazar_ArmTemplate_0')]"
			]
		}
	]
}