{
	"name": "PL_CARGA_INCREMENTAL",
	"properties": {
		"activities": [
			{
				"name": "CargaIncremental DAtos Hacia Datalake",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"sqlReaderQuery": {
							"value": "SELECT idFactura,idCliente,idEmpleado,fechaFactura,subtotal,impuestos \nROM dbo.Factura \nWHERE EOMONTH(fechaFactura)=@{formatDateTime(pipeline().parameters.FechaCorte, 'yyyy-dd-MM')}\n",
							"type": "Expression"
						},
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						},
						"formatSettings": {
							"type": "DelimitedTextWriteSettings",
							"quoteAllText": true,
							"fileExtension": ".txt"
						}
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "SQL_LEER",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "CSV_LEER",
						"type": "DatasetReference",
						"parameters": {
							"directorio": "/Taller/dbo/Factura/",
							"nombreArchivo": {
								"value": "@concat('Factura -',pipeline().parameters.FechaCorte,'.CSV')",
								"type": "Expression"
							}
						}
					}
				]
			}
		],
		"parameters": {
			"FechaCorte": {
				"type": "string"
			}
		},
		"folder": {
			"name": "CLASE03"
		},
		"annotations": []
	}
}